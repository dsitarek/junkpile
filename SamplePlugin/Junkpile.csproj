<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="Dalamud.Plugin.Bootstrap.targets" />

  <PropertyGroup>
    <Version>1.0</Version>
    <Description>Discard Junk items</Description>
    <PackageProjectUrl>https://github.com/dsitarek/junkpile</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="ClickLib">
      <HintPath>..\..\ClickLib\ClickLib\bin\Debug\ClickLib.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>
